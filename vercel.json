{
  "version": 2,
  "builds": [
    {
      "src": "api/app.py",
      "use": "@vercel/python"
    },
    {
      "src": "index.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "^/process$",
      "dest": "api/app.py"
    },
    {
      "src": "^/(index\\.html)?$",
      "dest": "/index.html"
    },
    {
      "src": "^/(.*)$",
      "dest": "/$1"
    }
  ]
}